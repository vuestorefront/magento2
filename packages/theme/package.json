{
  "name": "@vue-storefront/magento-theme",
  "version": "1.0.0-beta.7",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nuxt build -m",
    "build:analyze": "nuxt build -a -m",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "precommit": "lint-staged",
    "start": "nuxt start",
    "test": "jest",
    "test:e2e": "cypress open --config-file tests/e2e/cypress.json",
    "test:e2e:generate:report": "yarn -s mochawesome-merge \"tests/e2e/report/*.json\" > \"tests/e2e/report.json\" && yarn -s marge tests/e2e/report.json -o \"tests/e2e/report\"",
    "test:e2e:hl": "cypress run --headless --config-file tests/e2e/cypress.json",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "@storefront-ui/vue": "^0.10.5",
    "@vue-storefront/core": "^2.3.1",
    "@vue-storefront/magento": "^1.0.0-beta.7",
    "@vue-storefront/middleware": "^2.3.1",
    "@vue-storefront/nuxt": "^2.3.1",
    "@vue-storefront/nuxt-theme": "^2.3.1",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.12.1",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "nuxt": "^2.15.5",
    "nuxt-i18n": "^6.27.0",
    "vee-validate": "^3.4.5",
    "vue-dompurify-html": "^2.3.0",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-scrollto": "^2.20.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxtjs/pwa": "^3.3.5",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.3.0",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "^0.0.21",
    "dotenv": "^9.0.2",
    "ejs": "^3.1.6",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-localstorage-mock": "^2.4.12",
    "jest-silent-reporter": "^0.5.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^11.0.0",
    "majestic": "^1.8.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "npm-check-updates": "^11.5.11",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.1.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vue-jest": "^4.0.0-0",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "@vue/composition-api": ">= 1.0.0"
  }
}
