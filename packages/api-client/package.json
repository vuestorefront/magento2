{
  "name": "@vue-storefront/magento-api",
  "version": "2.0.1",
  "sideEffects": false,
  "homepage": "https://github.com/vuestorefront/magento2",
  "bugs": {
    "url": "https://github.com/vuestorefront/magento2/issues"
  },
  "license": "MIT",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib server && rollup -c",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest",
    "test:integration": "echo 'No test step created for this package'",
    "lint": "eslint ./src --ext .ts,.vue",
    "prepublish": "yarn build",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "partial:types": "node ./possible-types.js",
    "precommit": "lint-staged",
    "update:check": "ncu",
    "update:update": "ncu -u",
    "dev": "concurrently \"yarn build:package --watch\" \"yarn build:server --watch\""
  },
  "dependencies": {
    "@vue-storefront/middleware": "3.0.0",
    "@apollo/client": "^3.6.9",
    "@nuxtjs/composition-api": "0.31.0",
    "agentkeepalive": "^4.2.1",
    "consola": "^3.1.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@vue-storefront/magento-types": "1.0.0",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@graphql-tools/utils": "^8.6.1",
    "@types/graphql": "^14.5.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/webpack": "^5.28.0",
    "concurrently": "^7.2.2",
    "cross-fetch": "^3.1.5",
    "graphql-tools": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-graphql": "^2.1.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^0.43.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "webpack": "4.46.0"
  },
  "files": [
    "lib/**/*",
    "server/**/*",
    "types/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.x"
  }
}
