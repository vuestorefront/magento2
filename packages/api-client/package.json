{
  "name": "@vue-storefront/magento-api",
  "version": "1.2.0",
  "sideEffects": false,
  "homepage": "https://github.com/vuestorefront/magento2",
  "bugs": {
    "url": "https://github.com/vuestorefront/magento2/issues"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./server": "./lib/index.server.js",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:clear && yarn build:package && yarn build:server && yarn build:types",
    "build:clear": "rimraf lib",
    "build:package": "node ../../scripts/build --file=src/index.ts",
    "build:server": "node ../../scripts/build --file=src/index.server.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "concurrently \"yarn build:package --watch\" \"yarn build:server --watch\"",
    "lint": "eslint ./src --ext .ts,.vue",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "partial:types": "node ./possible-types.js",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "test": "jest",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@vue-storefront/core": "2.7.5",
    "@nuxtjs/composition-api": "0.31.0",
    "@apollo/client": "^3.6.9",
    "@vue-storefront/middleware": "3.0.0-rc.2",
    "agentkeepalive": "^4.2.1",
    "consola": "^2.15.3",
    "dotenv": "^16.0.1",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@graphql-tools/utils": "^8.6.1",
    "@types/graphql": "^14.5.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/webpack": "^5.28.0",
    "concurrently": "^7.2.2",
    "cross-fetch": "^3.1.5",
    "graphql-tools": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-graphql": "^2.1.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^0.43.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "webpack": "4.46.0",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@graphql-tools/utils": "^8.6.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/webpack": "^5.28.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^2.2.2",
    "jest-watch-toggle-config": "^2.0.1"
  },
  "files": [
    "lib/**/*",
    "server/**/*",
    "types/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.x"
  }
}
