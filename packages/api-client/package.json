{
  "name": "@vue-storefront/magento-api",
  "version": "1.0.0-rc.5.4",
  "sideEffects": false,
  "homepage": "https://github.com/vuestorefront/magento2",
  "bugs": {
    "url": "https://github.com/vuestorefront/magento2/issues"
  },
  "license": "MIT",
  "server": "server/index.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib server && rollup -c",
    "dev": "rimraf lib server && rollup -c -w",
    "lint": "eslint ./src --ext .ts,.vue",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "partial:types": "node ./possible-types.js",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "test": "jest",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "isomorphic-fetch": "^3.0.0",
    "dotenv": "^15.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-graphql": "^1.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "cross-fetch": "^3.1.5",
    "graphql-tools": "^8.2.0",
    "jest-transform-graphql": "^2.1.0",
    "msw": "^0.36.8",
    "typescript": "^4.5.5",
    "webpack": "4.46.0"
  },
  "peerDependencies": {
    "@vue-storefront/core": "~2.5.4"
  },
  "files": [
    "lib/**/*",
    "server/**/*",
    "types/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.x"
  }
}
