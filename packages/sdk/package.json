{
  "name": "@vue-storefront/magento-sdk",
  "version": "1.0.0",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "files": ["lib"],
  "scripts": {
    "build": "rimraf lib server && rollup -c",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest ./unit -c ./jest.config.unit.ts",
    "test:integration": "jest ./integration -c ./jest.config.integration.ts --runInBand",
    "lint": "eslint . --ext .ts",
    "prepublish": "yarn build",
    "dev": "rimraf lib server && rollup -c -w",
    "test:watch": "jest --watch",
    "test:coverage": "yarn test --coverage --passWithNoTests",
    "test:integration:init": "rm -rf __tests__/integration/__nock-fixtures__ && jest ./integration -c ./jest.config.integration.js --runInBand"
  },
  "dependencies": {
    "@vue-storefront/sdk": "^1.0.0",
    "axios": "^0.27.2",
    "ts-essentials": "^9.3.1",
    "@vue-storefront/magento-types": "^1.0.0"
  },
  "devDependencies": {
    "@vue-storefront/middleware": "3.0.0",
    "nock": "^13.2.9",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@babel/core": "^7.10.5",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.12.14",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^10.0.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^8.4.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2",
    "webpack-bundle-analyzer": "^3.5.2",
    "@apollo/client": "^3.6.9"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.x"
  }
}
